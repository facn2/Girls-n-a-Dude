{{#each data}}
   <div class="row">
         <div class="profile">
            <div class="thumbnail">
               <img src="{{this.image}}" class="img-responsive">
               <div class="name">
                  <h4>{{this.first_name}}</h4>
                  <h4>{{this.last_name}}</h4>

                  <p class="github">{{this.github}}</p>

                     <a href="#" class="btn btn-success pull-right" role="button">view more</a>
                  </div>
               </div>
            </div>
         </div>
{{/each}}
